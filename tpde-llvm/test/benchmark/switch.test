# RUN: python3 %s 2000 | tpde-llc --target=x86_64 | %objdump | FileCheck %s
# RUN: python3 %s 2000 | tpde-llc --target=aarch64 | %objdump | FileCheck %s

# CHECK: <f>:

import sys

n = int(sys.argv[1])
print("define void @f(i64 %c, ptr %p) {")
print("  switch i64 %c, label %d [")
for i in range(n):
    print(f"    i64 {i}, label %h{i}")
print(f"  ]")
for i in range(n):
    print(f'h{i}:')
    print(f'  store i64 {i*i}, ptr %p')
    print(f'  ret void')
print('d:')
print('  ret void')
print('}')
