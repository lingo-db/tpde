# Test for storing a large vector constant

# CHECK: <f>:

import sys

n = int(sys.argv[1])
print("define void @f(ptr %p, ptr %q) {")
print(f'  store <{n} x i32> zeroinitializer, ptr %q')
print('  ret void')
print('}')
